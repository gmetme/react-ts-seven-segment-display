{
  "name": "react-ts-seven-segment-display",
  "version": "1.1.0",
  "description": "A simple seven-segment display component built with React and TypeScript.",
  "keywords": [
    "react",
    "typescript",
    "seven-segment",
    "seven",
    "segment",
    "display",
    "digit",
    "digital clock"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "prepare": "npm run build:esm && npm run build:cjs",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/begenchmyrat/react-ts-seven-segment-display.git"
  },
  "author": "begenchmyratmeredamanov@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@types/prop-types": "^15.7.14",
    "@types/react": "^19.0.12",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "^5.8.2"
  }
}